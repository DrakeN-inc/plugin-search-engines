{
  "manifest_version": 3,
  "name": "FlatIcon search engine",
  "short_name": "flaticon-search",
  "description": "Adds the FlatIcon as search engine",
  "version": "0.0.1",
  "author": "DrakeN-inc",
  "icons": {
    "16": "icons/icon.svg",
    "64": "icons/icon.svg",
    "256": "icons/icon.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "FlatIcon",
      "keyword": "@flaticon",
      "search_url": "https://www.flaticon.com/search?word={searchTerms}"
    }
  },
  "homepage_url": "https://github.com/DrakeN-inc/plugin-search-engines/tree/main/flaticon",
  "browser_specific_settings": {
    "gecko": {
      "id": "flaticon-search@DrakeN-inc",
      "update_url": "https://github.com/DrakeN-inc/plugin-search-engines/tree/main/flaticon/updates.json"
    }
  }
}

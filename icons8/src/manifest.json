{
  "manifest_version": 3,
  "name": "Icons8 search engine",
  "short_name": "icons8-search",
  "description": "Adds the Icons8 as search engine",
  "version": "0.0.1",
  "author": "DrakeN-inc",
  "icons": {
    "16": "icons/icon.svg",
    "64": "icons/icon.svg",
    "256": "icons/icon.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Icons8",
      "keyword": "@icons8",
      "search_url": "https://icons8.ru/icons/set/{searchTerms}"
    }
  },
  "homepage_url": "https://github.com/DrakeN-inc/plugin-search-engines/tree/main/icons8",
  "browser_specific_settings": {
    "gecko": {
      "id": "icons8-search@DrakeN-inc",
      "update_url": "https://github.com/DrakeN-inc/plugin-search-engines/tree/main/icons8/updates.json"
    }
  }
}
